#include <iostream>
using namespace std;

int main() {
	int count = 1;
	while (count < 11) {
		cout << count << ' ';
		count++;
	}
	cout << '\n';

	int count2 = 1;
	while (count2 <= 10) {
		cout << count2 << ' ';
		count2++;
	}
	cout << '\n';

	for (int i = 1; i < 100; i += 2) {
		cout << i << ' ';
	}
	cout << '\n';
	
	for (int i = 4; i <= 100; i = i + 3) {
		cout << i << ' ';
	}
	cout << '\n';
}
